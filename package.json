{
  "name": "npm-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:all": "cypress run --config video=true --env allure=true",
    "allure:gener": "allure generate allure-results --clean -o allure-report",
    "allure:clean": "(if exist allure-results (rmdir /S /Q allure-results && mkdir allure-results))",
    "allure:history": "move allure-report/history allure-results/history",
    "test": "npm-run-all allure:gener allure:clean allure:history cy:all && allure open"
  },
  "keywords": [],
