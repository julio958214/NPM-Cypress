  "dependencies": {
    "cypress": "^9.7.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.26.5",
    "eslint-plugin-cypress": "^2.12.1",
    "install": "^0.13.0",
    "npm-run-all": "^4.1.5"
  }
}
